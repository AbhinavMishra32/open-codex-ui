{
  "name": "open-codex-ui",
  "version": "0.1.0-alpha",
  "description": "Codex-style macOS UI built with Electron, focused on strong architecture and extensible agent workflows.",
  "private": false,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "npx tsx main.ts",
    "ui": "next dev",
    "electron": "nodemon --watch src/electron/electron-app.ts --watch src/core --ext ts,js --exec \"NODE_OPTIONS='--import tsx' electron src/electron/electron-app.ts\"",
    "start": "concurrently -k \"npm run ui\" \"wait-on http://localhost:3000 && npm run electron\"",
    "release:check": "node ./scripts/release/check-release-ready.mjs",
    "release:notes": "node ./scripts/release/extract-changelog.mjs v$npm_package_version"
  },
  "dependencies": {
    "@ai-sdk/openai": "^3.0.21",
    "@ai-sdk/provider": "^3.0.5",
    "@ai-sdk/provider-utils": "^4.0.9",
    "@anthropic-ai/sdk": "^0.72.1",
    "@google/genai": "^1.39.0",
    "@langchain/core": "^1.1.23",
    "@langchain/langgraph": "^1.1.4",
    "@langchain/mcp-adapters": "^1.1.2",
    "@langchain/openai": "^1.2.3",
    "@openai/openai": "npm:@jsr/openai__openai@^6.16.0",
    "@tavily/core": "^0.7.1",
    "@types/node": "^25.0.10",
    "ai": "^6.0.57",
    "ink": "^6.6.0",
    "ink-markdown": "^1.0.4",
    "json-schema": "^0.4.0",
    "langchain": "^1.2.14",
    "next": "^16.1.6",
    "playwright": "^1.58.2",
    "react": "^19.2.4",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/json-schema": "^7.0.15",
    "@types/react": "^19.2.10",
    "autoprefixer": "^10.4.24",
    "concurrently": "^9.2.1",
    "electron": "^40.4.1",
    "electron-reloader": "^1.2.3",
    "nodemon": "^3.1.11",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3",
    "typescript-language-server": "^5.1.3",
    "wait-on": "^9.0.4"
  }
}
